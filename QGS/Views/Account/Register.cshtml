    <script>
        var msg = "@TempData["RegistrationFail"]"
        var msg2 = "@TempData["MailAlreadyExists"]"

        if (msg != '') {
            swal("Registration Failed !", "Try Again!", "error");
        }
        if (msg2 != '') {
            swal("Email ID Already Exists!", "Please Try with different Email Id!", "warning");
        }
    </script>

